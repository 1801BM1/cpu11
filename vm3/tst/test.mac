entry:		mov	#003117, @#177714	; 0 000	011 001	001 111
		mov	#003177, @#177716	; 0 000	011 001	111 111
		mov	#$stack, SP		;
						;
		mov	#$rxbuf, $rxend		;
		mov	#$rxbuf, $rxbeg		;
		mov	#$txbuf, $txend		;
		mov	#$txbuf, $txbeg		;
						;
		clr	R0			;
		mov	R0, @#140000		; bus timeout test
godt:						;
		mov	#100, @#RXCSR		;
		mov	#000, @#TXCSR		;
		clr	R0			;
		mtps	R0			;
